{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "content": {
      "type": "richtext"
    },
    "excerpt": {
      "type": "string"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category"
    },
    "tags": {
      "type": "string",
      "repeatable": true
    },
    "featuredImage": {
      "type": "media",
      "multiple": false
    },
    "infographics": {
      "type": "media",
      "multiple": true
    },
    "backlinks": {
      "type": "string",
      "repeatable": true
    },
    "externalLinks": {
      "type": "string",
      "repeatable": true
    },
    "funnelType": {
      "type": "enumeration",
      "enum": [
        "form",
        "ebook",
        "calendar",
        "none"
      ]
    },
    "funnelValue": {
      "type": "string"
    }
  }
}
